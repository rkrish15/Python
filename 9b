infile=open("/media/yughu/D/input.dat","r")
outfile=open("/media/yughu/D/output.dat","w")
sum=0
s = infile.read()
numbers = [int(x) for x in s.split()]
print("The numbers are:")
print(numbers)
for num in numbers:
 sum=sum+num
sum=str(sum)
outfile.write("Sum is ")
outfile.write(sum)
infile.close() 
outfile.close()
