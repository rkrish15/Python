def strlen(str): 
 counter = 0
 while str[counter:]: 
 counter += 1
 return counter 
 
def strrev(str): 
 rstr=""
 l=strlen(str)
 while l>0:
 rstr = rstr + str[l-1]
 l=l-1
 return rstr
 
def strcat(st1,st2):
 return(st1+st2)
 
def strcmp(st1,st2):
 if(st1==st2):
 print(st1 + " and " + st2 + " are same")
 elif (st1>st2):
 print(st1 + " comes after " + st2 +" in the Dictionary")
 else:
 print(st1 + " comes before " + st2 +" in the Dictionary")
